{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "declaration": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "skipLibCheck": true,
    "incremental": false,
    "tsBuildInfoFile": null
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".langgraph_api",
    "**/*.test.ts",
    "**/*.spec.ts"
  ]
}