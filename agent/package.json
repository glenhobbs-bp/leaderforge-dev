    "@langchain/core": "^0.3.21",
    "@langchain/langgraph": "^0.2.19",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.19.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
